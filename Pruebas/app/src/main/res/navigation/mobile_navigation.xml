<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_gestion">

    <fragment
        android:id="@+id/navigation_gestion"
        android:name="com.example.pruebas.Gestion"
        android:label="@string/gestion"
        tools:layout="@layout/fragment_gestion" >
        <action
            android:id="@+id/action_navigation_gestion_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_seguridad"
        android:name="com.example.pruebas.Seguridad"
        android:label="@string/seguridad"
        tools:layout="@layout/fragment_seguridad" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.pruebas.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />

</navigation>